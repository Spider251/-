    function win_open(url) {
        window.location.href = url
    };
    /*需要调整 判断导航哪个变颜色*/
    function navFn(parent) {
        $("" + parent + " li").removeClass("hover");
        $("" + parent + " li a").each(function (i) {
            var currnt;
            if (i < 7) {
                currnt = $(this).attr("href").split("/")[2];
                if ((location.href.indexOf(currnt) > -1 && location.href.indexOf("topic") < 0 && location.href.indexOf("photo") < 0)||currnt =="duoweicn.dwnews.com") {
                    $(this).parent().addClass("hover")
                }
            } else {
                currnt = $(this).attr("href");
                if (location.href.indexOf(currnt) > -1 && location.href.indexOf("photo") < 0) {
                    $(this).parent().addClass("hover")
                }
            }
        })
    }
