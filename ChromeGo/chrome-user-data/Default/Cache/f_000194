            $(".floatbar .fk").click(function () {
                $(".fkyy,.fk-box").css({ "display": "block" });
                $("[name='message']").val("")
            });
            $(".fk-box a").click(function () {
                $(".fkyy,.fk-box,.fksr span").css({ "display": "none" });
            }).mouseover(function () {
                $(this).css({ "background": "url(http://s0.dwnews.net/v5/images/cent-cha.png) no-repeat top -25px left 0" })
            }).mouseout(function () {
                $(this).css({ "background": "url(http://s0.dwnews.net/v5/images/cent-cha.png) no-repeat top 0 left 0" })
            });
            function sendEmail() {
                var message = $("[name='message']").val(),
                    body = "Message:" + "%0a%0d" + message;
                $("#send").attr("href", "mailto:newsdesk@dwnews.com?body=" + body);
                document.getElementById("send").click();
            }
            $(".btn").click(function () {
                if ($(".fksr textarea").val().length < 10) {
                    $(".fksr span").css({ "display": "block" });
                    $(".fksr textarea").focus()
                } else {
                    sendEmail()
                }
            });
            $(window).resize(function () {
                headerJs.fankui();
                $(".fk-box").css({ "margin-left": -$(".fk-box").width() / 2, "margin-top": -$(".fk-box").height() / 2 });
            })
            //当滚动条的位置处于距顶部200像素以下时，跳转链接出现，导航吸顶，否则消失
            $(window).on('scroll', publicJs.goBack);
            //当点击回到顶部，回到页面顶部位置
            $(".floatbar .goBack").click(function () {
                $('body,html').animate({ scrollTop: 0 }, 300);
                return false;
            });
            $(".headPublish").click(function () {
                //var that=this;
                if ($.cookie('dwUid') && $.cookie('dwUsername')) {
                    if (commonJs.registerData.regex[0].test($.cookie('dwEmail'))) {
                        /*=======================================*/
                        window.open("" + dwnews.dw5_blog + "/index.php?r=article%2Fpublish");
                        /*if($(that).find("a").attr("href")=="javascript:;"){
                         $(that).find("a").attr("href",""+dwnews.dw5_blog+"/index.php?r=article%2Fpublish");
                         $(that).find("a")[0].click();
                         }*/
                        /*=======================================*/
                    } else {
                        var c = new Layer();
                        c.loadLayer("" + language.complete + "！", '.bindmain');
                        publicJs.bdReg(".cent-ts", ".centBox");
                    }
                } else {
                    commonJs.loginLayer();
                }
            })
