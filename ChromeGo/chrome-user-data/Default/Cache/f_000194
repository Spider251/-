                $(".footer>ul>li").eq(3).find('dl dd a').eq(0).attr("href", "http://www.dwnews.com/copyright.html");
                $(".footer>ul>li").eq(3).find('dl dd a').eq(1).attr("href", "http://www.dwnews.com/privacy.html");
                $(".footer>ul>li").eq(3).find('dl dd a').eq(2).attr("href", "http://www.dwnews.com/disclaimer.html");
            };
            $(".floatbar .QRCode,.floatbar .QRCode img,.floatbar .bar-box").mouseenter(function () {
                $(".floatbar .bar-box").show();
            })
            $(".floatbar .QRCode,.floatbar .bar-box").mouseleave(function () {
                $(".floatbar .bar-box").hide();
            })
            $(".barbox .left-barbox dd").each(function (i) {
                $(this).hover(function () {
                    $(".barbox .left-barbox dd").removeClass("hover");
                    $(this).addClass("hover");
                    $(".barbox .right-barbox").addClass("none")
                    $(".barbox .right-barbox").eq(i).removeClass("none")
                })
            });
