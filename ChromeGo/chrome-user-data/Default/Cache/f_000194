		var t = DW5.DW5TYPE().type;
        var local = "首页";
		switch(t){
			case "0": local = "首页";break;
			case "1": local = "新闻";break;
			case "2": local = "经济";break;
			case "3": local = "人文";break;
			case "4": local = "对话";break;
			case "5": local = "普通内文";break;
			case "6": local = "图集内文";break;
			case "7": local = "多维客首页";break;
			case "8": local = "多维客内文";break;
		}
		return local;
